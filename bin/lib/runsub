#!/bin/bash

progname=$(basename $0)
subcommand=$1

case $subcommand in
    "" | "-h" | "--help")
        sub_default
        ;;
    *)
        shift
        sub_${subcommand} $@
        if [ $? = 127 ]; then
            echo "Error: '$subcommand' is not a known subcommand." >&2
            echo "       Run '$progname --help' for a list of known subcommands." >&2
            exit 1
        fi
        ;;
esac
